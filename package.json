{
  "name": "joker",
  "version": "0.2.1",
  "description": "application for testing module 2",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tactikerl/joker.git"
  },
  "keywords": [
    "test",
    "module",
    "2",
    "github"
  ],
  "author": "Karl Kvammen <karlmkvammen@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tactikerl/joker/issues"
  },
  "homepage": "https://github.com/Tactikerl/joker#readme",
  "devDependencies": {
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
